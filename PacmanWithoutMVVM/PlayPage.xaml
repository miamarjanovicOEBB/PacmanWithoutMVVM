        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="2.5*"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <Grid Background="Black" Opacity="0.8">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition Width="0.5*"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="0.8*"/>
                <RowDefinition Height="10*"/>
            </Grid.RowDefinitions>
            <Button Name="btn_menu" Content="r" Background="#FFFFD100" FontFamily="Webdings" FontSize="16" Click="btn_menu_Click" >
                <Button.Effect>
                    <DropShadowEffect/>
                </Button.Effect>
                <Button.Template>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" CornerRadius="18">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Border>
                    </ControlTemplate>
                </Button.Template>
            </Button>
            <Button x:Name="btn_pauseGame" Grid.Column="1" Content=";" Background="#FFFFD100" FontFamily="Webdings" FontSize="18" FontWeight="Bold" Click="btn_pauseGame_Click" >
                <Button.Effect>
                    <DropShadowEffect/>
                </Button.Effect>
                <Button.Template>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" CornerRadius="18">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Border>
                    </ControlTemplate>
                </Button.Template>
            </Button>
        </Grid>
        <Grid Grid.Column="2" Background="Black" Opacity="0.8">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="40"/>
                <RowDefinition Height="40"/>
                <RowDefinition Height="40"/>
                <RowDefinition Height="40"/>
                <RowDefinition Height="96*"/>
                <RowDefinition Height="235*"/>
            </Grid.RowDefinitions>
            <Label x:Name="txtScore"  Grid.Column="1" Opacity="0.9" Foreground="White" FontFamily="Bauhaus 93" FontSize="22" Margin="0,0,0,39" Grid.RowSpan="2">
                <Run Language="de-de" Text="00000"/>
            </Label>
            <TextBlock x:Name="txtBlock_timer" Grid.Column="1" Grid.Row="1" TextWrapping="Wrap" FontFamily="Bauhaus 93" FontSize="22" Foreground="White"><Run Language="de-de" Text="00"/></TextBlock>
            <TextBlock x:Name="txtbox_Life" Grid.Column="1" Grid.Row="1" TextWrapping="Wrap" Foreground="White" FontFamily="Bauhaus 93" FontSize="22" Margin="0,39,0,0" Grid.RowSpan="2"><Run Language="de-de" Text="3"/></TextBlock>
            <Label Content="SCORE" Background="{x:Null}" FontFamily="Bauhaus 93" FontSize="22" Foreground="White" Grid.ColumnSpan="2" Margin="0,0,89,39" Grid.RowSpan="2"/>
            <Label Content="TIME" Grid.Row="1" Background="{x:Null}" FontFamily="Bauhaus 93" FontSize="22" Foreground="White" Grid.ColumnSpan="2" Margin="0,0,89,0"/>
